{
  "name": "react-sorcerer",
  "version": "2.0.9",
  "description": "react-sorcerer",
  "main": "lib/Sorcerer.js",
  "author": "Bryan Brophy",
  "homepage": "https://github.com/brybrophy/react-sorcerer",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brybrophy/react-sorcerer.git"
  },
  "bugs": {
    "url": "https://github.com/brybrophy/react-sorcerer/issues"
  },
  "dependencies": {
    "classnames": "^2.1.2",
    "gulp": "^3.9.0",
    "gulp-responsive": "^2.7.0",
    "merge-stream": "^1.0.1"
  },
  "devDependencies": {
    "babel-eslint": "^4.1.3",
    "eslint": "^1.6.0",
    "eslint-plugin-react": "^3.5.1",
    "react": "^0.14.0",
    "react-component-gulp-tasks": "^0.7.6",
    "react-dom": "^0.14.0"
  },
  "peerDependencies": {
    "react": "^0.14.0"
  },
  "browserify-shim": {
    "react": "global:React"
  },
  "scripts": {
    "build": "gulp clean && NODE_ENV=production gulp build && gulp processImages",
    "examples": "gulp dev:server",
    "lint": "eslint ./; true",
    "publish:site": "NODE_ENV=production gulp publish:examples",
    "release": "NODE_ENV=production gulp release",
    "release:beta": "NODE_ENV=production gulp build && npm publish --tag beta",
    "start": "gulp dev",
    "test": "echo \"no tests yet\" && exit 0",
    "watch": "gulp watch:lib",
    "version": "npm run build && git add -A dist",
    "postversion": "git push origin HEAD && git push --tags"
  },
  "keywords": [
    "react",
    "react-component",
    "image",
    "img",
    "source",
    "src",
    "source set",
    "srcset"
  ],
  "directories": {
    "example": "example"
  },
  "license": "MIT"
}
